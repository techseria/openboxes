    <view-state id="createShipment" view="/createShipmentWorkflow/createShipment/enterContainerDetails">
        <transition on="success" to="enterShipmentDetails"/>
        <transition on="enterShipmentDetails" to="enterShipmentDetails" />
        <transition on="enterTrackingDetails" to="enterTrackingDetails" />
        <transition on="enterContainerDetails" to="enterContainerDetails" />
        <transition on="pickShipmentItems" to="pickShipmentItems" />
        <transition on="sendShipment" to="sendShipment" />
        <transition on="showDetails" to="showDetails" />
    </view-state>

    <view-state id="enterShipmentDetails" />
    <view-state id="enterTrackingDetails" />
    <view-state id="enterContainerDetails" />
    <view-state id="pickShipmentItems" />
    <view-state id="sendShipment" />
    <view-state id="showDetails" />

</flow>